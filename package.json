{
  "name": "vietjet-data",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "client:build": "cd client && yarn install && yarn pro:build",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nodemon",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js -f ./src/config/typeorm/migration.ts",
    "db:seed": "ts-node -r tsconfig-paths/register src/database/seeds/index.ts",
    "gen": "ts-node src/generator/generate-typing.ts",
    "combine": "ts-node src/generator/combineGraphql.ts"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/graphql": "^7.9.10",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/platform-fastify": "^7.6.13",
    "@nestjs/serve-static": "^2.1.4",
    "@nestjs/typeorm": "^7.1.5",
    "apollo-server-cache-memcached": "^0.6.7",
    "apollo-server-core": "^2.21.0",
    "apollo-server-express": "^2.21.0",
    "apollo-server-fastify": "^2.21.0",
    "babel-eslint": "^10.1.0",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "class-transformer": "^0.4.0",
    "compression": "^1.7.4",
    "dateformat": "^4.5.1",
    "dotenv": "^8.2.0",
    "express-rate-limit": "^5.2.6",
    "fastify-compress": "^3.4.1",
    "fastify-helmet": "^5.2.0",
    "fastify-rate-limit": "^5.4.1",
    "graphql": "^15.5.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-subscriptions": "^1.2.0",
    "graphql-tools": "^7.0.4",
    "graphql-type-json": "^0.3.2",
    "graphql-voyager": "^1.0.0-rc.31",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.4.1",
    "passport-azure-ad": "^4.3.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "typeorm": "^0.2.31",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@graphql-tools/load-files": "^6.3.1",
    "@graphql-tools/merge": "^6.2.10",
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/express": "^4.17.3",
    "@types/jest": "26.0.10",
    "@types/node": "^13.9.1",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "26.4.2",
    "nodemon": "^2.0.7",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}